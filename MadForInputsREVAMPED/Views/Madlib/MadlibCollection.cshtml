@model IEnumerable<Madlib>
@{
    ViewData["Title"] = "Collection";
}

<body>
    <form asp-controller="Madlib" asp-action="SearchMadlib" method="post">
        <input type="text" id="txtSearch" name="txtSearch" placeholder="Search for a Madlib..."/>
        <button type="submit" class="btn btn-success">Search</button>
    </form>

    <form asp-controller="Madlib" asp-action="FilterMadlibs" method="post">
        <select name="FilterByGenre" id="FilterByGenre">

        </select>

        <select name="FilterByDate" id="FilterByDate">

        </select>
        
        <select name="FilterByTitle" id="FilterByTitle">

        </select>
        <button type="submit" class="btn btn-success">Filter</button>
    </form>
    
    <div class="row">
        @foreach (Madlib madlib in Model)
        {
            <div class="col-sm-12 col-md-6 col-lg-4 text-center mb-3">
                <div id="padding-bot"class="display-4 text-center info">
                    @Model.Count() Madlib(s) found:
                </div>
                @if (madlib.Genre.ToLower() == "random")
                {
                    <img src="data:image/png;base64,"
                         width="200"
                         height="140"/>
                }
                else if (madlib.Genre.ToLower() == "action")
                {
                    <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTAkyLZP95e7arvYXADw9YfoDTdNknwZ_ItRg&usqp=CAU"
                         width="200"
                         height="140"/>
                }
                else if (madlib.Genre.ToLower() == "romance")
                {
                    <img src="data:image/jpeg;base64,"
                         width="200"
                         height="140"/>
                }
                else if (madlib.Genre.ToLower() == "comedy")
                {
                    <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcSBPsc1vZu2OIFJ82YIMRDHAQsyAHDQi9ENsQ&usqp=CAU"
                         width="200"
                         height="140"/>
                }
                else if (madlib.Genre.ToLower() == "horror")
                {
                    <img src="data:image/jpeg;base64,"
                         width="200"
                         height="140"/>
                }
                <partial name="_MadlibPartial" model="madlib"/>
                <form method="post" asp-controller="Madlib" asp-action="DisplaySelectedMadLib" asp-for="@madlib">
                    <input type="hidden" asp-for="@madlib.Id" />
                    <input type="hidden" asp-for="@madlib.Title" />
                    <input type="hidden" asp-for="@madlib.AuthorId" />
                    <input type="hidden" asp-for="@madlib.DatePublish" />
                    <input type="hidden" asp-for="@madlib.Story" />
                    <input type="hidden" asp-for="@madlib.Genre" />
                    <button type="submit">Read</button>
                </form>
            </div>
        }
    </div>
</body>